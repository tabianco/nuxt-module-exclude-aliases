{
  "name": "@tabianco/nuxt-module-exclude-aliases",
  "version": "1.0.1",
  "description": "Nuxt3 module to exclude specified aliases from alias config",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tabianco/nuxt-module-exclude-aliases.git"
  },
  "author": "Tabian Co.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tabianco/nuxt-module-exclude-aliases/issues"
  },
  "homepage": "https://github.com/tabianco/nuxt-module-exclude-aliases/tree/main#readme",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "run-s clean lint build:only build:fix",
    "build:only": "tsc",
    "build:fix": "eslint --fix --ext .js dist",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:types": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@nuxt/kit": "^3.0.0-rc.11"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.6",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "eslint": "^8.25.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4"
  }
}
